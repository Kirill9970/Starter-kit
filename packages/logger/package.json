{
  "name": "@crypton-nestjs-kit/logger",
  "version": "1.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "tsc-version": "tsc --version",
    "node-version": "node --version",
    "check-engines": "node ../../node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "clean": "rimraf build && rimraf coverage",
    "prebuild": "npm run clean && npm run check-engines",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test:ci",
    "test:ci": "jest --ci --collectCoverage --maxWorkers 2 --passWithNoTests"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.2.x"
  },
  "devEngines": {
    "node": ">=20.x",
    "npm": ">=10.2.x"
  },
  "dependencies": {
    "@crypton-nestjs-kit/config": "workspace:*",
    "@crypton-nestjs-kit/common": "workspace:*",
    "@types/luxon": "^3.4.2",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.4.15",
    "@nestjs/testing": "10.4.15",
    "@nestjs/typeorm": "^10.0.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/propagator-jaeger": "^1.9.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.3.2",
    "express": "^4.19.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.2.2"
  }
}
